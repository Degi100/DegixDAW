{
  "name": "frontend",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup:issues": "node scripts/setup-issues-table.js",
    "api": "node server/api.js",
    "app": "concurrently \"npm run dev\" \"npm run api\" --names \"dev,api\" --prefix-colors \"cyan,green\"",
    "db:migrate": "node scripts/db/migrate.js",
    "db:seed": "node scripts/db/seed.js",
    "db:cleanup": "node scripts/db/cleanup.js",
    "db:mark-done": "node scripts/db/mark-issues-done-v2.js",
    "db:sql": "node scripts/db/run-sql.js",
    "db:show": "node scripts/db/show-sql.js",
    "db:sync-profiles": "node scripts/db/show-sql.js scripts/sql/sync_profiles_auth_users.sql",
    "db:fix-profiles-rls": "node scripts/db/show-sql.js scripts/sql/fix_profiles_rls.sql",
    "db:social-setup": "node scripts/db/show-sql.js scripts/sql/social_connections_setup.sql",
    "db:debug-profiles": "node scripts/db/show-sql.js scripts/sql/debug_orphaned_profiles.sql",
    "db:create-profiles": "node scripts/db/show-sql.js scripts/sql/create_missing_profiles.sql",
    "db:fix-profile-ids": "node scripts/db/show-sql.js scripts/sql/fix_profile_ids.sql",
    "db:chat-setup": "node scripts/db/show-sql.js scripts/sql/chat_system_setup.sql"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.2",
    "react-spinners": "^0.17.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
